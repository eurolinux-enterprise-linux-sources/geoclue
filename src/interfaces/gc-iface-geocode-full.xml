		        <arg name="fields" type="i" direction="out" /> 
		        <arg name="latitude" type="d" direction="out" /> 
		        <arg name="longitude" type="d" direction="out" />
		        <arg name="altitude" type="d" direction="out" />

                        <arg name="accuracy" type="(idd)" direction="out" />
		</method>
	</interface>
</node>
